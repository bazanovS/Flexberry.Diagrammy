<h3 class="ui header">{{t "forms.i-i-s-diagrammy-заказ-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерЗаказа
      class=(if (v-get validationModel "номерЗаказа" "isInvalid") "error")
      label=(t "forms.i-i-s-diagrammy-заказ-e.номерЗаказа-caption")
      data-test-i-i-s-diagrammy-заказ-e-номерЗаказа=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номерЗаказа" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.фИОЗаказчика
      class=(if (v-get validationModel "фИОЗаказчика" "isInvalid") "error")
      label=(t "forms.i-i-s-diagrammy-заказ-e.фИОЗаказчика-caption")
      data-test-i-i-s-diagrammy-заказ-e-фИОЗаказчика=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "фИОЗаказчика" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.адресЗаказа
      class=(if (v-get validationModel "адресЗаказа" "isInvalid") "error")
      label=(t "forms.i-i-s-diagrammy-заказ-e.адресЗаказа-caption")
      data-test-i-i-s-diagrammy-заказ-e-адресЗаказа=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "адресЗаказа" "message")}}
  </div>
  <div class="field" data-test-i-i-s-diagrammy-заказ-e-способПолучения="true">
    <label>{{t "forms.i-i-s-diagrammy-заказ-e.способПолучения-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-diagrammy-доставка")
      value=model.способПолучения
      class=(if (v-get validationModel "способПолучения" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "способПолучения" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-diagrammy-заказ-e.датаПолучения-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датаПолучения
      class=(if (v-get validationModel "датаПолучения" "isInvalid") "error")
      data-test-i-i-s-diagrammy-заказ-e-датаПолучения=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаПолучения" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-diagrammy-заказ-e.тЧЗаказа-caption"}}</label>
    {{flexberry-groupedit
      componentName="тЧЗаказаGroupEdit"
      mainModelProjection=modelProjection
      modelProjection=modelProjection.attributes.тЧЗаказа
      content=model.тЧЗаказа
      readonly=readonly
      orderable=false
      class=(if (v-get validationModel "тЧЗаказа" "isInvalid") "error")
      data-test-i-i-s-diagrammy-заказ-e-тЧЗаказа=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "тЧЗаказа" "message")}}
  </div>
</form>
